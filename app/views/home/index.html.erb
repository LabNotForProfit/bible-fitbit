<div id='body'>
	<% if user_signed_in? %>
	  <h1>Welcome <%= current_user.email %>!</h1>
	  <h2>Coming Soon...</h2>
	  <li><strong>Friends</strong></li>
	  <li><strong>Progress</strong></li>
	  <li><strong>Leaderboards</strong></li>
	  <div class="datatable-wrapper">
			<table id="userNames">
				<thead>
					<tr>
						<th>User Name</th>
					</tr>
				</thead>
				<tbody>
					<% User.all.each do |user| %>
						<tr>
							<td><%= user.username %></td>
						</tr>
					<% end %>
				</tbody>
			</table>
	  </div>
	<% end %>
</div>